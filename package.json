{
	"name": "vscode-markdown-notebook",
	"publisher": "ms-vscode",
	"displayName": "Markdown Notebook",
	"description": "An extension for editing markdown files in VS Code notebooks",
	"version": "0.0.16",
	"preview": true,
	"enableProposedApi": true,
	"engines": {
		"vscode": "^1.54.0"
	},
	"repository": {
		"url": "https://github.com/microsoft/vscode-markdown-notebook"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"notebookProvider": [
			{
				"viewType": "markdown-notebook",
				"displayName": "Markdown Notebook",
				"priority": "option",
				"selector": [
					{
						"filenamePattern": "*.{md,markdown}"
					}
				]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"deploy": "npx vsce publish --noVerify",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.50.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"@types/js-yaml": "^4.0.0",
		"js-yaml": "^4.0.0"
	}
}
